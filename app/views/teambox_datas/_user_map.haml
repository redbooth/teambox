- user = users[user_map[0]]
%tr{:class => ''}
  %td
    %label{:for => "user_map_#{user_map[0]}"}= "#{user['first_name']} #{user['last_name']} (@#{user_map[0]})"
  %td=  select_tag "teambox_data[user_map][#{user_map[0]}]", options_for_select(options_for_user_map, user_map[1]), :id => "user_map_#{user_map[0]}"
  %td=  errors_for @data, "user_map_#{user_map[0]}"